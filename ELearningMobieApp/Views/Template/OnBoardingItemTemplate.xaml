<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ELearningMobieApp.Views.Template.OnBoardingItemTemplate">
    <StackLayout Spacing="35">
        <Image  HeightRequest="350" WidthRequest="350"  Aspect="AspectFit" HorizontalOptions="Center" Source="{Binding Image}"/>
        <Label Style="{StaticResource TitleLabel}" Text="{Binding Title}"/>
        <Label Style="{StaticResource SubTitleLabel}" Margin="20,0" Text="{Binding SubTitle}"/>
    </StackLayout>
</ContentView>

